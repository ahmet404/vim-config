  <script>
  window.onscroll = function() {scrollFunction()};
  function scrollFunction() {
    if (document.body.scrollTop > 50 || document.documentElement.scrollTop > 50) {
      document.getElementById("navigasi").style.background= "linear-gradient(45deg,#00349b,rgba(0,92,255,.94),#007aff)";
    } else {
      document.getElementById("navigasi").style.background= "transparent";
    }
  }
  </script>
